{
  "name": "PulseTile",
  "version": "0.1.0",
  "description": "Ripple OSI App UI built using Angular 1.5 and Redux",
  "homepage": "http://demo.rippleosi.org/",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PulseTile/PulseTile.git"
  },
  "author": "",
  "private": true,
  "engines": {
    "node": ">=6.2"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PulseTile/PulseTile/issues"
  },
  "scripts": {
    "commit": "git-cz",
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "build-plugins": "cross-env NODE_ENV=extension webpack --display-chunks --progress",
    "copy": "cross-env NODE_ENV=copy webpack --display-chunks --progress",
    "clean": "del-cli target",
    "lint": "eslint --cache .",
    "prebuild": "npm-run-all clean",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress --host 0.0.0.0",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "nyc-report-coverage": "nyc report --reporter=text-lcov | coverage.lcov && codecov",
    "cover": "nyc --reporter=lcov npm t",
    "validate": "npm run cover",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.10.4",
    "babel-eslint": "~6.1.0",
    "babel-loader": "~6.2.4",
    "babel-plugin-ng-annotate": "^0.3.2",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-polyfill": "~6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "~6.5.0",
    "babelify": "^7.3.0",
    "bootstrap-loader": "^1.1.6",
    "bower-webpack-plugin": "^0.1.9",
    "codecov.io": "^0.1.6",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "~1.0.8",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "eslint": "~2.13.1",
    "eslint-plugin-angular": "^1.3.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "^0.9.0",
    "ghooks": "^2.0.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "~2.21.0",
    "jasmine-core": "~2.4.1",
    "karma": "~1.1.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "~1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "~1.7.0",
    "lodash": "~4.13.1",
    "minx": "r-park/minx.git",
    "ng-annotate-loader": "^0.1.1",
    "ng-redux": "3.3.5-beta.1",
    "node-sass": "~3.8.0",
    "npm-run-all": "~2.2.2",
    "postcss-loader": "~0.9.1",
    "raw-loader": "~0.5.1",
    "redux": "~3.5.2",
    "redux-logger": "~2.6.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "~3.2.2",
    "sinon": "~1.17.4",
    "style-loader": "~0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.9",
    "angular-loading-bar": "^0.9.0",
    "angular-mocks": "~1.5.7",
    "angular-spinner": "^0.8.1",
    "angular-ui-bootstrap": "^2.1.3",
    "angular-ui-calendar": "^1.0.2",
    "angular-ui-router": "^1.0.0-beta.1",
    "angular-utils-pagination": "^0.11.1",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.5.0",
    "commitizen": "^2.9.5",
    "copy-webpack-plugin": "^3.0.1",
    "cz-conventional-changelog": "^1.2.0",
    "fullcalendar": "^3.0.0",
    "istanbul": "^0.4.5",
    "jquery": "~2.1.4",
    "karma-chai": "^0.1.0",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-mocha": "^1.3.0",
    "last-release-git": "0.0.2",
    "mocha": "^3.2.0",
    "moment": "~2.15.0",
    "nyc": "^10.0.0",
    "raphael": "^2.2.6",
    "redux": "^3.5.2",
    "semantic-release": "^6.3.2",
    "underscore": "^1.8.3"
  },
  "release": {
    "debug": false,
    "branch": "develop",
    "getLastRelease": "last-release-git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
